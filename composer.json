{
  "name": "juhniorsantos/record-processor",
  "description": "Record Processor",
  "keywords": [
    "record processor",
    "php"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Rodrigo Pedra Brum",
      "email": "rodrigo.pedra@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.3",
    "ext-pdo": "*",
    "ext-fileinfo": "*",
    "psr/log": "^3.0",
    "illuminate/support": "^10.0|^11.0",
    "illuminate/contracts": "^10.0|^11.0",
    "illuminate/filesystem": "^10.0|^11.0",
    "league/csv": "^9.21",
    "league/pipeline": "^1.1"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "symfony/var-dumper": "^3.3|~4.0|~4.1|~4.2|~4.3|~5.0",
    "symfony/console": "^3.3|~4.0|~4.1|~4.2|~4.3|~5.0",
    "symfony/process": "^3.3|~4.0|~4.1|~4.2|~4.3|~5.0",
    "pestphp/pest": "^1.23",
    "phpstan/phpstan": "^2.1",
    "laravel/pint": "^1.20",
    "mockery/mockery": "^1.6"
  },
  "autoload": {
    "files": [
      "src/RecordProcessor/helpers.php"
    ],
    "psr-4": {
      "RodrigoPedra\\RecordProcessor\\": "src/RecordProcessor/",
      "RodrigoPedra\\RecordProcessor\\Examples\\": "examples/"
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
